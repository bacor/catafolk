{
    "dataset_id": "densmore-pawnee",
    "file_pattern": "*.krn",
    "default_field_values": {
        "has_music": true,
        "has_lyrics": false
    },
    "override_field_values": {
        "source_key": "densmore1929pawnee",
        "preview_url": "https://verovio.humdrum.org/?file=osu/densmore/pawnee/{path}",
        "url": "https://kern.humdrum.org/cgi-bin/ksdata?location=osu/densmore/pawnee&file={path}&format=kern",
        "source_url": "https://books.google.com/books?id=-9e65i0Fu2kC&pg=PA{source_page_num}"
    },
    "meta_fields_conversion": {
        "OTL": "title_eng",
        "MLC": "location",
        "ODT": "collection_date",
        "MPN": "performer",
        "AST": "genre",
        "AMT": "metric_classification",
        "CNT": "culture",
        "OCL": "collector",
        "ENC": "encoder",
        "RDT": "encoding_date",
        "YEC": "copyright",
        "URL": "url",
        "EEV": {
            "type": "regex",
            "pattern": "Release ([\\d\\.]+)",
            "groups": ["version"]
        },
        "YOR": {
            "type": "regex",
            "pattern": "Bulletin 93, page (\\d+), No\\. (\\d+)",
            "groups": ["source_page_num", "source_song_num"]
        }
    }
}