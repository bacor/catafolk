{
    "dataset_id": "essen-china-han",
    "file_pattern": "*.krn",
    "file_encoding": "ISO-8859-1",
    "default_field_values": {
        "has_music": true,
        "has_lyrics": false,
        "culture": "Han"
    },
    "override_field_values": {
        "preview_url": "https://verovio.humdrum.org/?file=essen/asia/china/han/{path}",
        "url": "https://kern.humdrum.org/cgi-bin/ksdata?file={path}&l=essen/asia/china/han&format=kern"
    },
    "meta_fields_conversion": {
        "OTL": "title",
        "ARE": "location",
        "YEM": "copyright",
        "AMT": "metric_classification",
        "EED": "encoder",
        "EEV": "version",
        "AGN": "genre",
        "SCT": "catalogue_num"
    },
    "meta_values_conversion": {
    }
}