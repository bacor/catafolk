{
    "dataset_id": "essen-deutschl-altdeu1",
    "sources": [{
        "name": "file",
        "type": "file",
        "file_pattern": "data/*.krn"
    }],
    "transformations": [
        ["rename", "file.cf_checksum", "checksum"],
        ["rename", "file.cf_path", "path"],
        ["rename", "file.cf_format", "format"],
        ["constant", "dataset_id", "essen-deutschl-altdeu1"],
        ["constant", "has_music", true],
        ["constant", "has_lyrics", false],
        ["constant", "source_key", "bohme1877altdeutsches"],
        ["rename", "file.YEM", "copyright"],
        ["rename", "file.AMT", "metric_classification"],
        ["rename", "file.EED", "encoder"],
        ["rename", "file.EEV", "version"],
        ["rename", "file.AGN", "genre"],
        ["rename", "file.ARE", "location"],
        ["titlecase", "file.OTL", "title"],
        [
            "format", "path", "preview_url", 
            {"pattern": "https://verovio.humdrum.org/?file=essen/europa/deutschl/altdeu1/{}"}
        ],
        [
            "format", "path", "url", 
            {"pattern": "https://kern.humdrum.org/cgi-bin/ksdata?file={}&l=essen/europa/deutschl/altdeu1&format=kern"}
        ],
        {
            "operation": "extract_groups",
            "inputs": ["file.SCT"],
            "outputs": ["catalogue_num", "source_song_num"],
            "params": {
                "pattern": "A0*(\\d+\\w+)?",
                "groups": [0, 1]
            }
        }
    ]
}