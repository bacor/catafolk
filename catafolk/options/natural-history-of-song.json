{
    "dataset_id": "natural-history-of-song",
    "file_pattern": "Transcriptions/xml_raw/*.xml", 
    "metadata_tables": [
        {
            "path": "Datasets/NHSDiscography_Metadata.csv",
            "index_col": "song",
            "override_columns": []
        },
        {
            "path": "Datasets/NHSDiscography_TranscriptionFeatures.csv",
            "index_col": "song",
            "index_pattern": "NHSDiscography-{:0>3}"
        }
    ],
    "default_field_values": {

    },
    "override_field_values": {
    },
    "meta_fields_conversion": {
        "NHSDiscography_Metadata_type": "genre",
        "NHSDiscography_Metadata_culture": "culture",
        "NHSDiscography_Metadata_location_modern": "location",
        "NHSDiscography_Metadata_latitude": "latitude",
        "NHSDiscography_Metadata_longitude": "longitude",
        "NHSDiscography_Metadata_year": "collection_date",
        "NHSDiscography_Metadata_permalink": "source_url",
        "NHSDiscography_Metadata_citation": {
            "type": "regex",
            "pattern": "(\"+([^\"]+)\"+( +\\[([^\\]]+)\\])?\\.? ([^\\.]+)\\.( ([^:]+): ([^\\(]+)\\(([^\\)]+)\\))?)",
            "groups": [
                "source", 
                "title", 
                null,
                "source_song_num",
                "source_title",
                null,
                "source_address",
                "source_publisher",
                "source_date"
            ]
        },
        "NHSDiscography_TranscriptionFeatures_range": "ambitus"
    },
    "meta_values_conversion": {
    },
    "corrections": {
        "NHSDiscography-094": {
            "title": "Angi: Love song to a married woman",
            "source_song_num": "Disc 1, Side B, Track 10",
            "source_title": "On The Demonstration Collection of E.M. von Hornbostel and the Berlin Phonogramm-Archiv [LP]",
            "source_address": "New York",
            "source_publisher": "Folkways Records",
            "source_date": "1963"
        }
    }
}